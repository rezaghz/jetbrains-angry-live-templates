<templateSet group="AngryLiveTemplates">
    <!--لایو تمپلیت ها تو میتونی طبق استراکچر زیر وارد کنی -->
    <!--p aval name ha be manaye farsi boodan oone-->

    <!--ALLFILES CONTEXT-->
    <template name="kk"
              value="kirekhar$END$"
              description="Kire Khar TEXT"
              toReformat="false"
              toShortenFQNames="false">
        <context>
            <option name="ALLFILES" value="true"/>
        </context>
    </template>
    <template name="pkk"
              value="کیرخر$END$"
              description="Kirekhar"
              toReformat="false"
              toShortenFQNames="false">
        <context>
            <option name="ALLFILES" value="true"/>
        </context>
    </template>
    <template name="ktd"
              value="kiram too in davari $END$"
              description="kiram too in davari"
              toReformat="true"
              toShortenFQNames="false">
        <context>
            <option name="ALLFILES" value="true"/>
        </context>
    </template>
    <template name="pktd"
              value="کیرم تو این داوری $END$"
              description="کیرم تو این داوری"
              toReformat="true"
              toShortenFQNames="false">
        <context>
            <option name="ALLFILES" value="true"/>
        </context>
    </template>
    <template name="ktk"
              value="kir too karfarma $END$"
              description="kir too karfarma"
              toReformat="true"
              toShortenFQNames="false">
        <context>
            <option name="ALLFILES" value="true"/>
        </context>
    </template>
    <template name="pktk"
              value="کیر تو کارفرما $END$"
              description="کیر تو کارفرما"
              toReformat="true"
              toShortenFQNames="false">
        <context>
            <option name="ALLFILES" value="true"/>
        </context>
    </template>

    <!--JAVASCRIPT CONTEXT-->
    <template name="ckk"
              value="console.log('kirekhar');$END$"
              description="console.log('kirekhar');"
              toReformat="false"
              toShortenFQNames="false">
        <context>
            <option name="JSFILES" value="true"/>
        </context>
    </template>


    <!--PHP CONTEXT-->
    <template name="ekk"
              value="echo 'kirekhar';$END$"
              description="echo 'kirekhar';"
              toReformat="false"
              toShortenFQNames="false">
        <context>
            <option name="PHPFILES" value="true"/>
        </context>
    </template>

    <!--LARAVEL CONTEXT-->
    <template name="dkk"
              value="dd('kirekhar');$END$"
              description="dd('kirekhar');"
              toReformat="false"
              toShortenFQNames="false">
        <context>
            <option name="LARAVELFILES" value="true"/>
        </context>
    </template>

</templateSet>